{
  "name": "iter8-mcp-server",
  "version": "2.1.0",
  "description": "iter8敏捷团队AI代理MCP服务器，为Augment Code提供深度集成支持，兼容MCP 1.0+",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "iter8-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "prepare": "npm run build",
    "install-global": "npm run build && npm link"
  },
  "keywords": [
    "iter8",
    "agile",
    "mcp",
    "model-context-protocol",
    "augment-code",
    "ai-agent",
    "封神演义",
    "敏捷开发",
    "cursor-ide"
  ],
  "author": "iter8 Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "js-yaml": "^4.1.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/iter8_agent.git",
    "directory": ".iter8/integrations/augment-code/mcp-server"
  },
  "bugs": {
    "url": "https://github.com/your-org/iter8_agent/issues"
  },
  "homepage": "https://github.com/your-org/iter8_agent#readme",
  "mcp": {
    "name": "iter8-agile-team",
    "description": "iter8敏捷团队AI代理系统MCP服务器",
    "version": "2.1.0",
    "capabilities": {
      "tools": true,
      "resources": true,
      "prompts": true,
      "logging": true
    },
    "oauth": {
      "supported": true,
      "providers": [
        "github",
        "jira",
        "slack"
      ]
    }
  }
}
